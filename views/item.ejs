<!DOCTYPE html>
<html>
  <%- include("layout/head") %> 
    <body>
        <%- include("layout/nav") %> 
            <% item.forEach(item => { %>
                <div class="p-5">
                    <img class="logo w-10" src="https://pbs.twimg.com/profile_images/1100594156718702593/_p_SyfuH_400x400.png" alt="">
                    <h2><%= item.name %></h2>
                    <p><%= item.desc %></p>
                    <p>quantity: <%= item.quantity %></p>
                    <p><%= item.price %>$</p>
                    <button class="btn">BUY</button>
                </div>
            <% }); %>
            </div>
        </div>
    </body>
</html>